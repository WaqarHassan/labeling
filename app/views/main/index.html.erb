<div class="approval-data tspend" id="ghzanfer">
<a href="#" class="minus-icon"></a>
  <table class="table table-bordered">
    <thead>
      <tr class="main_heading">
        <th>IA Approval</th>
        <th colspan="3" style="text-align:center">Translation</th>
        <th colspan="2" style="text-align:center">ECR</th>
        <th colspan="4" style="text-align:center">Collaboration</th>
      </tr>
      <tr class="sub_heading">
        <th>IA Approved</th>
        <th>Inbox</th>
        <th>Sent</th>
        <th>Received</th>
        <th>Inbox  </th>
        <th>Complete  </th>
        <th>Sent to Collab.  </th>
        <th>Back from Coll.  </th>
        <th>Sent to Legal  </th>
        <th>Back from Legal </th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @projects.each do |project| %>
        <% project.ias.each do |ia| %>
          <% ia_approved = ia.inbox_date %>
          <% ia.ecrs.each do |ecr| %>
            <tr>
              <td>
                <%= link_to ia_approved, open_modal2_path,  {:remote => true, 'data-toggle' =>  "modal", 'data-target' => '#info_modal_popup2'} %>
              </td>
              <td></td>
              <td></td>
              <td></td>
              <td>
                <%= link_to ecr.created_at.strftime("%Y-%m-%d %I:%M %p"), open_modal2_path,  {:remote => true, 'data-toggle' =>  "modal", 'data-target' => '#info_modal_popup2'} %>
              </td>
              <td>
                <%= link_to ecr.created_at.strftime("%Y-%m-%d %I:%M %p"), open_modal2_path,  {:remote => true, 'data-toggle' =>  "modal", 'data-target' => '#info_modal_popup2'} %>
              </td>
              <td>
                <%= link_to '5/19/2016  1:18:00 PM', open_modal3_path,  {:remote => true, 'data-toggle' =>  "modal", 'data-target' => '#info_modal_popup3'} %>
                </td>
              <td>
                <%= link_to '5/19/2016  1:18:00 PM', open_modal3_path,  {:remote => true, 'data-toggle' =>  "modal", 'data-target' => '#info_modal_popup3'} %>
              </td>
              <td>MM/DD hh:mm</td>
              <td>MM/DD hh:mm</td>
              <td>MM/DD hh:mm</td>
            </tr>
          <% end %>
        <% end %>
      <% end %>
    </tbody>
  </table>
 </div> 
