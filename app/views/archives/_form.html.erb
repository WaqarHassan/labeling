<%= simple_form_for(@archive) do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :id %>
    <%= f.input :ecr_id %>
    <%= f.input :stored_on %>
    <%= f.input :user_id %>
    <%= f.input :number_of_component %>
    <%= f.input :updated_by %>
    <%= f.input :orignal_value %>
  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
