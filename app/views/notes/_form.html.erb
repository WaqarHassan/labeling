<%= simple_form_for(@note) do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :id %>
    <%= f.input :ecr_id %>
    <%= f.input :created_by %>
    <%= f.input :updated_by %>
    <%= f.input :description %>
  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
